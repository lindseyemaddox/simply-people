
$primary-font = 'Source Sans Pro'
$performance-font = 'Helvetica', 'Arial', sans-serif
$primary-font-size = 16px
$primary-font-line-height = 1.5em
$light = 300
$normal = 400
$semibold = 600
$bold = 700

primary-font(w)
  font-family: $primary-font, $performance-font
  font-weight: w
  line-height: $primary-font-line-height

html
  font-size: $primary-font-size

@font-face {
    font-family: 'simply-people';
    src:    url('../_fonts/simply-people.eot?mbvisy');
    src:    url('../_fonts/simply-people.eot?mbvisy#iefix') format('embedded-opentype'),
        url('../_fonts/simply-people.ttf?mbvisy') format('truetype'),
        url('../_fonts/simply-people.woff?mbvisy') format('woff'),
        url('../_fonts/simply-people.svg?mbvisy#simply-people') format('svg');
    font-weight: normal;
    font-style: normal;
}

[class^="icon-"], [class*=" icon-"] {
    /* use !important to prevent issues with browser extensions that change fonts */
    font-family: 'simply-people' !important;
    speak: none;
    font-style: normal;
    font-weight: normal;
    font-variant: normal;
    text-transform: none;
    line-height: 1;

    /* Better Font Rendering =========== */
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}

